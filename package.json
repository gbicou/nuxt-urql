{
  "name": "@bicou/nuxt-urql",
  "version": "1.11.1",
  "description": "Nuxt URQL module",
  "license": "MIT",
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/gbicou/nuxt-urql.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "default": "./dist/module.mjs"
    }
  },
  "main": "dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "nuxi prepare playground",
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxi prepare playground",
    "dev:build": "nuxi build playground",
    "dev:start": "nuxi start playground",
    "build": "nuxt-module-build build",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "clean": "rimraf dist/ playground/.nuxt/ playground/.output/ playground/node_modules/.vite/"
  },
  "dependencies": {
    "@urql/core": "^5.1.1",
    "@urql/vue": "^1.4.3"
  },
  "devDependencies": {
    "@bicou/countries-server-schema": "1.7.1",
    "@nuxt/kit": "3.17.3",
    "@nuxt/module-builder": "1.0.1",
    "@nuxt/schema": "3.17.3",
    "@nuxt/test-utils": "3.18.0",
    "@types/node": "22.15.18",
    "@urql/exchange-execute": "2.3.1",
    "defu": "6.1.4",
    "eslint": "9.26.0",
    "graphql": "16.11.0",
    "nuxt": "3.17.3",
    "typescript": "5.8.3",
    "vitest": "3.1.3"
  },
  "peerDependencies": {
    "defu": "^6.1.4",
    "graphql": "^16.10.0",
    "vue": "^3.5.13"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",
  "publishConfig": {
    "access": "public"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "vue-demi"
    ]
  }
}

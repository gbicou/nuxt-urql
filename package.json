{
  "name": "@bicou/nuxt-urql",
  "version": "1.9.0",
  "description": "Nuxt URQL module",
  "license": "MIT",
  "author": "Benjamin VIELLARD <bicou@bicou.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/gbicou/nuxt-urql.git"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "nuxi prepare playground",
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxi prepare playground",
    "dev:build": "nuxi build playground",
    "dev:start": "nuxi start playground",
    "build": "nuxt-module-build build",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "clean": "rimraf dist/ playground/.nuxt/ playground/.output/ playground/node_modules/.vite/",
    "release": "changeset publish"
  },
  "dependencies": {
    "@nuxt/kit": "3.13.2",
    "@urql/core": "5.0.6",
    "@urql/vue": "1.4.1"
  },
  "devDependencies": {
    "@bicou/countries-server-schema": "1.5.0",
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.9",
    "@graphql-eslint/eslint-plugin": "3.20.1",
    "@nuxt/eslint-config": "0.6.0",
    "@nuxt/module-builder": "0.8.4",
    "@nuxt/schema": "3.13.2",
    "@nuxt/test-utils": "3.14.3",
    "@types/node": "20.16.11",
    "@urql/exchange-execute": "2.3.0",
    "@vueuse/shared": "11.1.0",
    "defu": "6.1.4",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsdoc": "50.4.1",
    "eslint-plugin-prettier": "5.2.1",
    "graphql": "16.9.0",
    "nuxt": "3.13.2",
    "prettier": "3.3.3",
    "vitest": "2.1.3",
    "wonka": "6.3.4"
  },
  "peerDependencies": {
    "defu": "6.1.4",
    "graphql": "16.9.0",
    "vue": "3.5.12"
  },
  "packageManager": "pnpm@9.12.1+sha512.e5a7e52a4183a02d5931057f7a0dbff9d5e9ce3161e33fa68ae392125b79282a8a8a470a51dfc8a0ed86221442eb2fb57019b0990ed24fab519bf0e1bc5ccfc4",
  "publishConfig": {
    "access": "public"
  }
}

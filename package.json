    "clean": "rimraf dist/ playground/.nuxt/ playground/.output/ playground/node_modules/.vite/",
    "release": "changeset publish"
  },
  "dependencies": {
    "@urql/core": "^5.1.0",
    "@urql/vue": "^1.4.2"
  },
  "devDependencies": {
    "@nuxt/kit": "3.15.3",
